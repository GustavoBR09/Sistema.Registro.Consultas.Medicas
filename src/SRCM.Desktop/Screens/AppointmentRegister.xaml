    <Grid>
        <Label x:Name="LabelPatient" Content="Cadastro de &#xA;Consultas" HorizontalAlignment="Left" Margin="44,15,0,0" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="43" Width="302" Height="110" Background="White" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-1.002"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.525"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="RegisterNewAppointment" Content="Cadastrar e fazer novo cadastro" HorizontalAlignment="Left" Height="43" Margin="327,383,0,0" VerticalAlignment="Top" Width="291" BorderBrush="#FFDDDDDD" RenderTransformOrigin="1,0" BorderThickness="1,1,1,1" Click="Button_Click" FontFamily="Arial Black" FontSize="16">
